{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
      "import/extensions": "off",
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "off",
      "no-return-assign": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "quotes": ["warn", "backtick"]
    }
  }